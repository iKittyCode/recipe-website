    "ingredients": [
        "2 slices bread",
        "1 teaspoon butter",
        "arugula",
        "2  sweet gherkin pickles",
        "3 slices smoked ham",
        "2 slices white Cheddar cheese",
        "1 small tomato",
        "1 teaspoon mustard"
    ],
    "method": [
        "Step 1: Lightly butter both slices of bread.",
        "Step 2: Layer the arugula, pickles, ham, Cheddar, and tomatoes on one slice of buttered bread. Spread the mustard on the remaining buttered bread slice, and place on top. Cut sandwich in half, and serve."
