    "ingredients": [
        "2 cups chopped kale",
        "1  banana",
        "½ cup light unsweetened soy milk",
        "1 tablespoon flax seeds",
        "1 teaspoon maple syrup"
    ],
    "method": [
        "Step 1: Gather all ingredients.",
        "Step 2: Place kale, banana, soy milk, flax seeds, and maple syrup into a blender.",
        "Step 3: Cover and purée until smooth.",
        "Step 4: Serve over ice."
