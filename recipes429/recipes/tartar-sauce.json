    "ingredients": [
        "1 cup mayonnaise",
        "1 tablespoon sweet pickle relish",
        "1 tablespoon minced onion",
        "2 tablespoons lemon juice (Optional)",
        "salt and pepper to taste"
    ],
    "method": [
        "Step 1: Gather all ingredients.",
        "Step 2: Mix mayonnaise, relish, and onion together in a small bowl.",
        "Step 3: Stir in lemon juice; season with salt and pepper.",
