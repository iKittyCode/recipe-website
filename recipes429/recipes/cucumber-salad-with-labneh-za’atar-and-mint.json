    "ingredients": [
        "1/4 cup labneh",
        "1/4 cup fresh mint,",
        "2 tablespoons za'atar spice",
        "1 1/2 tablespoons lemon juice",
        "1 tablespoon extra-virgin olive oil",
        "1/2 teaspoon salt",
        "1/2 teaspoon black pepper",
        "2  English cucumbers",
        "1 teaspoon lemon zest"
    ],
    "method": [
        "Step 1: In a bowl, stir together labneh, mint, za’atar, lemon juice, oil, salt, and pepper. Stir in cucumbers; toss to coat. Garnish with lemon zest and additional fresh mint leaves."
