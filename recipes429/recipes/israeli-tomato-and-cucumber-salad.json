    "ingredients": [
        "4  English cucumbers, diced",
        "4  Roma (plum) tomatoes, seeded and diced",
        "1  red bell pepper, seeded and diced",
        "1 cup chopped fresh parsley",
        "½  red onion, diced",
        "3 tablespoons chopped fresh mint",
        "2 tablespoons chopped garlic",
        "½ cup olive oil",
        "2 tablespoons fresh lemon juice",
        "1 tablespoon kosher salt",
        "1 tablespoon ground black pepper"
    ],
    "method": [
        "Step 1: Toss cucumbers, tomatoes, bell pepper, parsley, onion, mint, and garlic together in a bowl. Drizzle olive oil and lemon juice over salad; toss to coat. Season with salt and black pepper."
