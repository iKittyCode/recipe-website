    "ingredients": [
        "¼ cup bulgur",
        "½ cup boiling water",
        "5  ripe tomatoes, finely diced",
        "1 small onion, finely diced",
        "1 cup freshly chopped parsley",
        "¼ cup freshly chopped mint leaves",
        "1  lemon, juiced",
        "2 teaspoons olive oil",
        "salt to taste"
    ],
    "method": [
        "Step 1: Place bulgur in a small mixing bowl. Add boiling water, mix, and cover with a clean towel. Let stand for 1 hour. Drain any excess water.",
